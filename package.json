{
  "name": "coffee-date-chronicles",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web-app"
  ],
  "scripts": {
    "prepare": "command -v husky >/dev/null 2>&1 && husky || echo 'Husky not found, skipping'",
    "build": "cd web-app && npm run build",
    "lint": "cd web-app && npm run lint",
    "lint:precommit": "cd web-app && npm run lint:precommit",
    "format": "cd web-app && npm run format"
  },
  "devDependencies": {
    "husky": "^9.0.11"
  }
}
